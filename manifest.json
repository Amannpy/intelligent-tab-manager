{
    "manifest_version": 3,
    "name": "Intelligent Tab Manager",
    "version": "1.0",
    "description": "An AI-powered Chrome extension to intelligently group browser tabs.",
    "permissions": [
      "tabs",
      "storage",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
}
  